<Alloy>
	<Collection src="schedules">
	<View class="container">
		<View id="customTabBar">
			<View id="invisibleTabA">
				<Label id="btn" text="events" />
			</View>
			<View id="invisibleTabB">
				<Label id="btn" text="notifications" />
			</View>
			<View id="buttonTab">
				<Label id="btn" text="schedule" />
			</View>
		</View>
		<View id="scheduleView">
			<TableView id="scheduleTable" dataCollection="schedules">
				<TableViewRow title="{title}" />
			</TableView>
		</View>
	</View>
</Alloy>